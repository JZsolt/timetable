{
  "name": "timetable",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "install:backend": "yarn --cwd backend install",
    "develop:backend": "yarn --cwd api develop",
    "develop:frontend": "wait-on http://localhost:1337/admin && yarn start",
    "develop": "cross-env FORCE_COLOR=1 npm-run-all -l -p develop:*",
    "build:backend": "yarn --cwd backend build",
    "build:all": "yarn build:backend && yarn build"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~13.3.0",
    "@angular/cdk": "^13.3.7",
    "@angular/common": "~13.3.0",
