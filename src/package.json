    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "npm-run-all": "4.1.5",
    "postcss": "^8.3.11",
    "prettier": "^2.4.1",
    "sass": "^1.43.4",
    "typed-scss-modules": "^4.1.1",
    "typescript": "~4.6.2",
    "wait-on": "5.2.1"
  }
